block b-page {

    default: {
        var bundleName = this.data.bundleName;

        return applyCtx({
            block : this.block,
            title : this.data.pageName,
            favicon : '/favicon.ico',
            head : [
                { elem : 'css', url : '_' + bundleName + '.css' },
                { block : 'i-jquery', elem : 'core' },
                { elem : 'js', url : '_' + bundleName + '.js' }
            ],
            content : this.ctx.content
        });
    }

}